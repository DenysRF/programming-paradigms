    This program should be correctly parsed.
    Contains complex operations like addition, subtraction, multiplication,
    comparisons (and boolean operators).
    Combining them and forcing priority with parentheses.
}$

int a <- 1 + 2 + 3;                     $  6
int b <- -4 + 5 - -6;                   $  7
int c <- 1 + 1 + 1 + 1 + 1 + 1 + 1 + 1; $  8
int d <- -1 - -1 + -1 - -1;             $  0

int e <- 2 + 2 * 3;                     $  8
int f <- (2+2) * 3;                     $ 12

bool g <- 5 - -1 * 3 = 18;              $ true
bool h <- -1 - 1 not= -1 * 2;           $ false
bool i <- (2 + 2 > 3 + 3) = true;       $ false

